{
  "name": "westures-example",
  "version": "1.0.0",
  "description": "Simple static example page of the Westures library in action",
  "author": "Michael van der Kamp <michael.vanderkamp@usask.ca>",
  "license": "ISC",
  "main": "dist/sample.js",
  "targets": {
    "main": {
      "context": "browser",
      "includeNodeModules": true,
      "isLibrary": false,
      "outputFormat": "global",
      "publicUrl": "./",
      "sourceMap": true
    }
  },
  "browserslist": [
    "> 0.2%",
    "not dead"
  ],
  "scripts": {
    "build": "parcel build sample.js",
    "postinstall": "rm -rf ./.parcel-cache && node ./postinstall.js",
    "start": "npx http-server"
  },
  "dependencies": {
    "westures": "^1.0.0-alpha.3"
  },
  "devDependencies": {
    "eslint": "^7.3.0",
    "http-server": "^0.12.3",
    "parcel": "^2.0.0-nightly.300"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvanderkamp/westures-example.git"
  },
  "keywords": [
    "westures",
    "example",
    "gesture",
    "library",
    "multitouch"
  ],
  "bugs": {
    "url": "https://github.com/mvanderkamp/westures-example/issues"
  },
  "homepage": "https://github.com/mvanderkamp/westures-example#readme"
}
